{
	"name": "myfolio",
	"version": "1.0.0",
	"description": "Personal generic portfolio website framework built with Vue 3 and Vite",
	"type": "module",
	"author": "Németh Bálint",
	"license": "MIT",
	"scripts": {
		"dev": "vite",
		"build": "vite build",
		"preview": "vite preview",
		"lint": "eslint . --ext .vue,.ts,.js",
