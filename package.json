		"@vue/eslint-config-typescript": "^14.6.0",
		"@vue/tsconfig": "^0.5.1",
		"eslint": "^9.39.2",
		"eslint-config-prettier": "^10.1.8",
		"eslint-plugin-vue": "^10.7.0",
		"prettier": "^3.8.1",
		"terser": "^5.46.0",
		"typescript": "^5.4.2",
		"typescript-eslint": "^8.54.0",
		"vite": "^5.1.6",
		"vue-eslint-parser": "^10.2.0",
		"vue-tsc": "^2.0.6"
